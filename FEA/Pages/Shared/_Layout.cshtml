<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FEA</title>

    <link rel="stylesheet" href="~/lib/twitter-bootstrap/css/bootstrap.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/ekko-lightbox/ekko-lightbox.css" asp-append-version="true">
    <link rel="stylesheet" href="~/lib/Chartjs/Chart.min.css" asp-append-version="true">

    <link href="//fonts.googleapis.com/css?family=Roboto:300,400" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body data-spy="scroll" data-target="#TopNavBar" data-offset="50">
    <div id="Top" class="container siteLogo">
        <div class="row">
            <div class="col text-center">
                <a asp-page="/">
                    <img src="~/images/FEALogoTeal.png" class="img-fluid" alt="FEA">
                </a>
            </div>
        </div>
        <div class="row">
            <div class="col text-center">
                <h1 class="sitelogo">
                    Specialist Support <i class="fas fa-chart-line"></i> and Executive Recruitment <i class="fas fa-users"></i> for Further Education Establishments <i class="fas fa-graduation-cap"></i>
                </h1>
            </div>
        </div>
    </div>
    <nav id="TopNavBar" class="navbar navbar-expand-lg navbar-dark bg-dark sticky-top scrollAnchor">
        <div class="container">
            <a class="navbar-brand" asp-page="/Index">
                <img src="~/images/FEALogoWhiteSmall.png" class="img-fluid" alt="FEA">
            </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item">
                        <a class="nav-link" asp-page="/Index">Home</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown1" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Services
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" href="~/Services/ExecutiveRecruitment">Executive Recruitment</a>
                            <a class="dropdown-item" href="~/Services/MISSpecialists">MIS Specialists</a>
                            <a class="dropdown-item" href="~/Services/FinanceSpecialists">Finance Specialists</a>
                            <a class="dropdown-item" href="~/Services/QualityImprovement">Quality Improvement</a>
                            <a class="dropdown-item" href="~/Services/OfstedReadiness">Ofsted Readiness</a>
                            <a class="dropdown-item" href="~/Services/StaffTraining">Staff Training</a>
                        </div>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown2" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Vacancies
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" href="#">MIS</a>
                            <a class="dropdown-item" href="#">Finance</a>
                            <a class="dropdown-item" href="#">Quality</a>
                            <a class="dropdown-item" href="#">Teaching</a>
                            <a class="dropdown-item" href="#">Other</a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item" href="#">Join Our Team as a Consultant</a>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-page="/News">Sector News</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-page="/About">Our Team</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-page="/Contact">Contact</a>
                    </li>
                </ul>
                <ul class="navbar-nav">
                    <li class="nav-item contact">
                        <a class="nav-link" href="tel:01454%20617%20707"><i class="fas fa-phone"></i> 01454 617 707</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>


    @*<partial name="_CookieConsentPartial" />*@

    <div class="body-content">
        @RenderBody()

        <div class="container-fluid background3">
            <div class="row">
                <div class="col-md">
                    <div class="container boxPadder">
                        <div class="row">
                            <div class="col-md text-center">
                                <footer>
                                    <p>Privacy Policy | Site Map</p>
                                    <p class="social"><i class="fab fa-twitter"></i> <i class="fab fa-linkedin"></i></p>
                                    <p>&copy; 2018-2021 - FEA</p>
                                    <p>Registered Office: FE Associates, 9 Apex Court, Woodlands, Bristol, BS32 4JT</p>
                                </footer>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/lib/twitter-bootstrap/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/lib/momentjs/moment-with-locales.min.js" asp-append-version="true"></script>
    <script src="~/lib/marked/marked.min.js" asp-append-version="true"></script>
    <script src="~/lib/ekko-lightbox/ekko-lightbox.min.js" asp-append-version="true"></script>
    <script src="~/lib/ScrollMagic/ScrollMagic.min.js" asp-append-version="true"></script>
    <script src="~/lib/Chartjs/Chart.min.js" asp-append-version="true"></script>

    <environment include="Development">
        <script src="~/lib/ScrollMagic/plugins/debug.addIndicators.js" asp-append-version="true"></script>
    </environment>

    <script src="~/js/palette.js" asp-append-version="true"></script>
    <script src="~/js/modals.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
